#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/Felipe Pedroza/Documents/UnB/REQ-2025.2-T01-Softex/docs-astro/node_modules/.pnpm/direction@2.0.1/node_modules/direction/node_modules:/mnt/c/Users/Felipe Pedroza/Documents/UnB/REQ-2025.2-T01-Softex/docs-astro/node_modules/.pnpm/direction@2.0.1/node_modules:/mnt/c/Users/Felipe Pedroza/Documents/UnB/REQ-2025.2-T01-Softex/docs-astro/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/Felipe Pedroza/Documents/UnB/REQ-2025.2-T01-Softex/docs-astro/node_modules/.pnpm/direction@2.0.1/node_modules/direction/node_modules:/mnt/c/Users/Felipe Pedroza/Documents/UnB/REQ-2025.2-T01-Softex/docs-astro/node_modules/.pnpm/direction@2.0.1/node_modules:/mnt/c/Users/Felipe Pedroza/Documents/UnB/REQ-2025.2-T01-Softex/docs-astro/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../direction/cli.js" "$@"
else
  exec node  "$basedir/../direction/cli.js" "$@"
fi
